<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.detectspycamera.detectspycamera">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="com.detectspycamera.detectspycamera.Outside" />
        <activity android:name="com.detectspycamera.detectspycamera.Changing_Room" />
        <activity android:name="com.detectspycamera.detectspycamera.Bathroom" />
        <activity android:name="com.detectspycamera.detectspycamera.Bedroom" />
        <activity android:name="com.detectspycamera.detectspycamera.CameraDetecter" />
        <activity android:name="com.detectspycamera.detectspycamera.CameraDetectorByGraph" />
        <activity android:name="com.detectspycamera.detectspycamera.Location" />
        <activity android:name="com.detectspycamera.detectspycamera.meterDetector" />
        <activity android:name="com.detectspycamera.detectspycamera.Infrared_Detection" />
        <activity android:name="com.detectspycamera.detectspycamera.CameraFinderSelection" />

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="@string/app_id" />

        <activity android:name="com.detectspycamera.detectspycamera.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>